{
    "name": "sajya/server",
    "description": "Local web server for the development of Laravel",
    "keywords": [
        "framework",
        "laravel",
        "server",
        "https"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Chernyaev",
            "email": "bliz48rus@gmail.com",
            "homepage": "https://tabuna.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "5.8.*",
        "react/http": "^0.8.4",
        "symfony/psr-http-message-bridge": "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sajya\\Server\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sajya\\Server\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sajya\\Server\\ServerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
